{
    "name": "@regex-radar/recheck-benchmark",
    "version": "4.5.0",
    "description": "A benchmark suite for recheck",
    "license": "MIT",
    "private": true,
    "author": {
        "name": "Kevin Ramharak",
        "url": "https://github.com/kevinramharak"
    },
    "homepage": "https://github.com/regex-radar/recheck-benchmark",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/regex-radar/recheck-benchmark.git"
    },
    "type": "module",
    "files": [],
    "scripts": {
        "build": "node scripts/build.js",
        "prebench": "npm run build",
        "bench": "node --experimental-wasm-exnref dist/index.js",
