    },
    "optionalDependencies": {
        "@regex-radar/recheck-scalajs": "^4.5.0",
        "@regex-radar/recheck-scalajs-wasm": "^4.5.0-alpha.1",
        "recheck-jar": "^4.5.0",
        "recheck-linux-arm64": "^4.5.0",
        "recheck-linux-x64": "^4.5.0",
        "recheck-macos-arm64": "^4.5.0",
        "recheck-macos-x64": "^4.5.0",
        "recheck-windows-x64": "^4.5.0",
        "synckit": "^0.11.11"
    },
    "devDependencies": {
        "@types/node": "^24.10.1",
        "esbuild": "^0.27.0",
        "prettier": "^3.6.2",
        "tinybench": "^5.1.0",
        "typescript": "^5.9.3"
